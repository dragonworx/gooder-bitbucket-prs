{
  "manifest_version": 2,
 
  "name": "Gooder Bitbucket PRs",
  "version": "1.2.0",
  "description": "Bring some goodness to Bitbucket Cloud PRs!",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["https://bitbucket.org/*"]
  }],

  "web_accessible_resources": ["file-A.png", "file-D.png", "file-M.png", "file-R.png", "file-C.png", "folder.png"],

  "icons": { "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" }
}